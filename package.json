{
  "name": "@sayari/trellis",
  "version": "0.7.0-rc.5",
  "description": "High-performance network visualization library, rendering to WebGL as well as other targets",
  "author": "James Conkling <james.lane.conkling@gmail.com> jameslaneconkling.github.io",
  "license": "ISC",
  "types": "./src/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .parcel-cache",
    "format": "prettier . --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/**.{ts,tsx}\" --fix",
    "perf": "parcel perf/index.html",
    "copy": "cp package* README.md dist/",
    "build:tsc": "tsc --project ./tsconfig.json",
    "build": "npm run clean && npm run build:tsc && parcel build",
    "build:analyze": "npm run clean && parcel build --detailed-report --reporter @parcel/reporter-bundle-analyzer",
    "examples:dev": "npm run clean && parcel examples/index.html",
    "docs:dev": "npm run clean && parcel docs-src/index.html",
    "docs:build": "rm -rf ./docs/ && parcel build ./docs-src/index.html --dist-dir docs/ --public-url https://sayari-analytics.github.io/trellis/ && cp ./docs-src/assets/og-image.png ./docs/"
  },
  "targets": {
    "types": false,
    "main": {
      "source": "src/index.ts",
      "outputFormat": "commonjs",
      "isLibrary": true
    },
    "module": {
      "source": "src/index.ts",
      "outputFormat": "esmodule",
      "isLibrary": true
    }
  },
  "exports": {
    ".": {
      "import": "./module/index.js",
      "require": "./main/index.js"
    },
    "./layout": "./src/layout/index.js",
    "./react": "./src/bindings/react/index.js",
    "./native": "./src/bindings/native/index.js",
    "./webgl": "./src/renderers/webgl/index.js",
    "./image": "./src/renderers/image/index.js"
  },
  "typesVersions": {
    "*": {
      "layout": ["./src/layout/index.d.ts"],
      "react": ["./src/bindings/react/index.d.ts"],
      "native": ["./src/bindings/native/index.d.ts"],
      "webgl": ["./src/renderers/webgl/index.d.ts"],
      "image": ["./src/renderers/image/index.d.ts"]
    }
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "dependencies": {
    "@pixi/unsafe-eval": "^6.5.5",
    "d3-color": "^3.1.0",
    "d3-force": "^3.0.0",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "fontfaceobserver": "^2.1.0",
    "pixi.js-legacy": "^6.5.5"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/reporter-bundle-analyzer": "^2.9.3",
    "@parcel/transformer-typescript-tsc": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "@turf/clusters-kmeans": "^6.3.0",
    "@types/d3-color": "^3.1.0",
    "@types/d3-force": "^3.0.5",
    "@types/d3-hierarchy": "^3.1.3",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-scale": "^4.0.4",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/fontfaceobserver": "0.0.6",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.8",
    "@types/react-resize-detector": "^5.0.0",
    "@types/stats.js": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "parcel": "^2.9.3",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-resize-detector": "^5.2.0",
    "rxjs": "^6.5.5",
    "stats.js": "^0.17.0",
    "ts-loader": "^6.2.2",
    "typescript": "^5.2.2"
  },
  "keywords": ["graph", "network", "infovis", "visualization", "react", "webgl"],
  "peerDependencies": {
    "react": ">=16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/junonetwork/graph.git"
  }
}
